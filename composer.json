{
  "name": "signes/acl",
  "description": "Advanced ACL for Laravel",
  "keywords": [
    "laravel",
    "acl",
    "groups",
    "roles"
  ],
  "homepage": "https://github.com/signes-pl/laravel-acl",
  "license": "MIT",
  "authors": [
    {
      "name": "Grzesiecki",
      "email": "pawel.grzesiecki@signes.pl"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "orchestra/testbench": "^3.1.0"
  },
  "require": {
    "php": ">=5.4.0",
    "illuminate/support": "^5.1",
    "illuminate/container": "^5.1",
    "illuminate/database": "^5.1",
    "illuminate/events": "^5.1"
  },
  "autoload": {
    "psr-4": {
      "Signes\\Acl\\": "src/Acl/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php",
      "src/Publish"
    ]
  },
  "minimum-stability": "stable"
}